@startuml domainmodel
    left to right direction
    skinparam classAttributeIconSize 0

    class Jogador {
        +nome
        +telefone
        +localização
        +posição
        +partidas
        +encontrarPartidas()
        +entrarPartida()
        +avaliarJogador()
    }

    class Organizador {
        +nome
        +telefone
        +partidas
        +criarPartida()
        +cancelarPartida()
        +gerenciarjogadores()
    }

    class Partida {
        +localização
        +horário
        +status
        +jogadores
        +addJogador()
        +removerJogador()
        +getJogadores()
    }

    Jogador "*" --* "*" Partida : joga >
    Organizador "1" -- "*" Partida : organiza >
    Organizador "*" -- "*" Jogador : gerencia >
@enduml

@startuml domainmodel
    left to right direction
    skinparam classAttributeIconSize 0

    class Jogador {
        +nome : str
        +telefone : str
        +localização : str
        +posição : str
        +partidas : array[Partida]
        +encontrarPartidas()
        +entrarPartida()
        +avaliarJogador()
    }

    class Organizador {
        +nome : str
        +telefone : str
        +partidas : array[Partida]
        +criarPartida()
        +cancelarPartida()
        +gerenciarjogadores()
    }

    class Partida {
        +localização : str
        +horário : DateTime
        +status : bool
        +jogadores : array[Jogador]
        +organizador : Organizador
        +addJogador()
        +removerJogador()
        +getJogadores()
    }

    Jogador "*" --* "*" Partida : joga >
    Organizador "1" -- "*" Partida : organiza >
    Organizador "*" -- "*" Jogador : gerencia >
@enduml
